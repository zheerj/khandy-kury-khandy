    "@types/morgan": "^1.7.35",
    "@types/node": "^10.5.1",
    "@types/node-cache": "^4.1.1",
    "@types/pg": "^7.4.10",
    "@types/shortid": "^0.0.29",
    "@types/winston": "^2.3.9",
    "apidoc": "^0.17.6",
    "chai": "^4.1.2",
    "chai-as-promised": "^7.1.1",
    "chai-http": "^4.0.0",
    "mocha": "^5.2.0",
    "prompt": "^1.0.0",
    "ts-node": "^7.0.0",
    "tslint": "^5.10.0",
    "typescript": "^2.9.2"
